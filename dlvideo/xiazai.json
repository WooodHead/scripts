{
    "default":
    {
        "out_dir": "%(home)s/Downloads"
    },
    "u2b":
    {
        "cmd": "%(home)s/bin/youtube-dl ",
        "proxy": "127.0.0.1:8087",
        "cache": ".u2cache",
        "out_dir": "%(home)s/download/%(title)s-%(id)s.%(ext)s",
        "creat_dir": false
    },
    "xiazai":
    {
        "cmd": "py %(home)s/code/pkhopper/youku-lixian/video_lixian.py ",
        "out_dir": "%(home)s/download",
        "creat_dir": false
    },
    "downloads":
    {
        "out_dir": "%(home)s/download",
        "lib_dir": "%(home)s/code/pkhopper/youku-lixian"
    }
}